{
  "name": "runtypes-generator",
  "version": "0.0.0",
  "description": "Compose and react upon redux actions",
  "main": "dist/index.js",
  "bin": {
    "rungen": "dist/cli.js"
  },
  "scripts": {
    "build": "yarn clean && tsc && rimraf 'dist/**/?(__tests__|__mocks__|__setup__|*.test.*)'",
    "clean": "rimraf dist",
    "start": "concurrently yarn:start:*",
    "start:build": "tsc --watch --preserveWatchOutput",
    "start:test": "yarn jest src --watch",
    "test": "yarn jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/runtypes-generator.git"
  },
  "keywords": [],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/runtypes-generator/issues"
  },
  "homepage": "https://github.com/johngeorgewright/runtypes-generator#readme",
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/jest": "26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "14.17.5",
    "commitizen": "4.2.4",
    "concurrently": "6.2.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "js-yaml": "^4.1.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "runtypes": "^6.3.1",
    "semantic-release": "17.4.4",
    "ts-jest": "27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "yarn prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@johngw/error": "^2.1.0",
    "ts-morph": "^11.0.3",
    "tslib": "2.3.0",
    "yargs": "^17.0.1"
  },
  "peerDependencies": {
    "runtypes": "*"
  }
}
