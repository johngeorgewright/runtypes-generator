{
  "name": "runtyping",
  "version": "4.1.1",
  "description": "Compose and react upon redux actions",
  "main": "dist/index.js",
  "bin": {
    "rungen": "dist/cli.js",
    "runtyping": "dist/cli.js"
  },
  "scripts": {
    "build": "yarn clean && tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "schema": "typescript-json-schema --out runtyping.schema.json src/runtypes.ts Instructions && prettier --write runtype.schema.json",
    "start": "concurrently --names build,test 'yarn start:build' 'yarn start:test'",
    "start:build": "tsc --watch --preserveWatchOutput",
    "start:test": "jest --watch",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/runtyping.git"
  },
  "keywords": [],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/runtyping/issues"
  },
  "homepage": "https://github.com/johngeorgewright/runtyping#readme",
  "devDependencies": {
    "@commitlint/cli": "13.2.0",
    "@commitlint/config-conventional": "13.2.0",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/github": "8.0.1",
    "@semantic-release/npm": "8.0.0",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/jest": "27.0.2",
    "@types/js-yaml": "4.0.3",
    "@types/node": "14.17.20",
    "@types/yargs": "17.0.3",
    "commitizen": "4.2.4",
    "concurrently": "6.2.2",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.2",
    "jest": "27.2.3",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "runtypes": "6.4.1",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "typescript-json-schema": "0.51.0"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "yarn prettier --write"
    ],
    "src/runtypes.ts": [
      "yarn schema"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "runtypes": "^6.4.0"
  },
  "dependencies": {
    "@johngw/array": "3.1.5",
    "@johngw/error": "2.1.1",
    "@johngw/iterator": "1.0.2",
    "js-yaml": "4.1.0",
    "json-schema-to-typescript": "10.1.5",
    "ts-morph": "12.0.0",
    "tslib": "2.3.1",
    "yargs": "17.2.1"
  }
}
