    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "28.1.6",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.48",
    "@types/prettier": "2.7.0",
    "@types/yargs": "17.0.11",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
